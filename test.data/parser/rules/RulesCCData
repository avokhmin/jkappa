#test01
A()
A(x~p!1), B(y~w!1, z!2), YUYI(m, u!2)
--------------

#test02
A01(x~p!0, y!1), A01(x~p!2, y!1), A01(x~p!2, y!3), A01(x~p!4, y!3), A01(x~p!6, y!5), A01(x~p!6, y!7), A01(x~p!8, y), B01(x!7, y!9, z!8), B01(x, y, z!0), B01(x~p!5, y!9, z!4)
----------

#test03
----------
A()

#test04
A()
-----------
A(x)

#testBlindUnbound01
A(x!_)
-----------
A(x)

#testBlindUnbound02
A(x!_, y)
-----------
A(x, y)

#testBlindUnbound03
A(x!_, y!_)
-----------
A(x, y!_)

#testBlindUnbound04
A(x!1, y!_), B(k!1)
-----------
A(x!1, y), B(k!1)

#testBlindUnbound05
A(x!1, y!_), B(k!1, l)
-----------
A(x!1, y), B(k!1, l)

#testBound01
A(x)
B(k)
-----------
A(x!1), B(k!1)

#testBound02
A(x)
B(k, l)
-----------
A(x!1), B(k!1, l)

#testBound03
A(x)
A(x)
-----------
A(x!1), A(x!1)

#testBound04
A(x, y)
B(k, l)
-----------
A(x!1, y), B(k!1, l)

#testBound05
A(x, y)
B(k, l!_)
-----------
A(x!1, y), B(k!1, l!_)

#testBound06
A(x, y!_)
B(k, l!_)
-----------
A(x!1, y!_), B(k!1, l!_)

