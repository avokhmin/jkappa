'rule_one' A(x, u), B(y) -> A(x!1, u), B(y!1) @ 1.0

%init: 20000 * (A(x, u, k!1), B(y, l!1))

%obs: 'A free' A(x)
%obs: 'B free' B(y)
%obs: 'initial' (A(x, u, k!0), B(y, l!0))
%obs: 'unary 1 generation' (A(x!0, u, k!1), B(y!0, l!1))
%obs: 'binary 1 generation' (A(x!0, u, k!1), B(y, l!1), A(x, u, k!1), B(y!0, l!1))
