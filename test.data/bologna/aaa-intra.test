'AA@x' A(x),A(x) <-> A(x!1),A(x!1) @ 1.0 (10000.0),100.0
'AA@y' A(y),A(y) <-> A(y!1),A(y!1) @ 1.0 (10000.0),100.0
'AA@z' A(z),A(z) <-> A(z!1),A(z!1) @ 1.0 (10000.0),100.0

%init: 30000 * A(x,y,z)
%obs: 'A.3.A' A(x!0,y!1,z!2),A(x!0,y!1,z!2)
%obs: 'A.2.A' A(x!1,y!2),A(x!1,y!2)
%obs: 'A.1.A' A(x!_,y,z)
%obs: 'A' A(x,y,z)


