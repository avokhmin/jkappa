A(x~u) <-> A(x~p) @1,0.1
A(x~p),B(x) -> A(x~p!1),B(x!1) @1
A(x!1),B(x!1) -> A(x),B(x) @5
A(x!1),B(x!1,y1~u) -> A(x!1),B(x!1,y1~p)  @1
B(y1~p) -> B(y1~u) @0.1
A(x!1),B(x!1,y2~u) -> A(x!1),B(x!1,y2~p) @1
B(y2~p) -> B(y2~u)  @0.1
'goal' B(y1~p,y2~p,z~u) -> B(y1~p,y2~p,z~p) @20 

%init: 5*(A(x~u),B(x,y1~u,y2~u,z~u))

%story: 'goal'

